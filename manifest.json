{
    "manifest_version": 3,
    "name": "Detect_Deactivated",
    "version": "1.0",
    "description": "Visually alert idle tabs and view usage stats.",
    "permissions": ["tabs", "storage"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  